# game/game_script.rpy:1712
translate ja game_script_cf1cc6d7:

    # mc "There {i}has{/i} to be a way."
    mc "絶対に…{i}方法はある。{/i}"

# game/game_script.rpy:1717
translate ja game_script_a51de77b_2:

    # n "[player]..."
    n "[player]…"

# game/game_script.rpy:1722
translate ja game_script_440ada8e:

    # mc "{i}I’m serious, Natsuki.{/i}"
    mc "{i}本気だよ、ナツキ。{/i}"

